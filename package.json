{
  "name": "timings",
  "version": "2.0.4",
  "description": "RESTful API for performance engineering",
  "keywords": [
    "perf",
    "perfeng",
    "performance"
  ],
  "repository": "https://github.com/godaddy/timings.git",
  "license": "MIT",
  "author": "GoDaddy Operating Company, LLC",
  "contributors": [
    "Marcel Verkerk <mverkerk@godaddy.com>"
  ],
  "main": "server.js",
  "bin": {
    "timings": "./server.js"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "git config core.hooksPath .git/hooks/ && npx simple-git-hooks",
    "semantic-release": "npx semantic-release --no-ci",
    "semantic-release:dry-run": "npx semantic-release --dry-run --no-ci --branches `git rev-parse --abbrev-ref HEAD`",
    "start": "node ./server",
    "test": "nyc --reporter=cobertura npm run test:mocha",
    "posttest": "npm run lint",
    "test:mocha": "mocha test/**/*.js --exit"
  },
  "simple-git-hooks": {
    "pre-commit": "echo running pre-commit hook && npm run lint",
    "commit-msg": "npx --no-install commitlint --edit $1",
    "pre-push": "echo running pre-push hook && npm test",
    "preserveUnused": false
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.17.0",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-winston": "^4.2.0",
    "form-data": "^4.0.0",
    "fs-extra": "^11.1.1",
    "git-rev": "^0.2.1",
    "is-docker": "^2.2.1",
    "joi": "^17.9.2",
    "js-yaml": "^4.1.0",
    "jsoneditor": "^9.10.2",
    "mime-types": "^2.1.35",
    "morgan": "^1.10.0",
    "nconf": "^0.12.0",
    "node-fetch": "^2.6.11",
    "parent-module": "^3.0.0",
    "percentile": "^1.6.0",
    "semver": "^7.5.1",
    "serve-favicon": "^2.5.0",
    "snyk": "^1.1159.0",
    "traceback": "^0.3.1",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^10.0.3",
    "chai": "^4.3.7",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.41.0",
    "eslint-config-godaddy": "^7.0.2",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-mocha": "^10.1.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.22",
    "nyc": "^15.1.0",
    "pinst": "^3.0.0",
    "semantic-release": "^21.0.2",
    "simple-git-hooks": "^2.8.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.10.0",
    "npm": ">=8.11.0"
  }
}
