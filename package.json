{
    "name": "timings",
    "version": "1.0.2",
    "description": "RESTful API for performance engineering",
    "scripts": {
        "start": "node ./server --env prod",
        "start-local": "node ./server --env local",
        "start-local-debug": "node --debug ./server --env local",
        "start-dev": "node ./server --env dev",
        "start-dev-debug": "node --debug ./server --env dev",
        "start-test": "node ./server --env test",
        "start-test-debug": "node --debug ./server --env test",
        "test": "mocha --recursive --timeout 30000",
        "lint": "eslint-godaddy public/"
    },
    "engines": {
        "node": ">=8.0.0",
        "npm": ">=5.0.0"
    },
    "main": "server.js",
    "bin": {
        "timings": "./server.js"
    },
    "keywords": [
        "perf",
        "perfeng",
        "performance"
    ],
    "repository": "godaddy/timings",
    "author": "GoDaddy Operating Company, LLC",
    "contributors": [
        "Marcel Verkerk <mverkerk@godaddy.com>"
    ],
    "license": "MIT",
    "dependencies": {
        "body-parser": "~1.0.0",
        "bootstrap": "^3.3.7",
        "config": "^1.21.0",
        "cookie-parser": "~1.0.1",
        "debug": "~0.7.4",
        "ejs": "^1.0.0",
        "elasticsearch": "~12.1.3",
        "express": "^4.2.0",
        "express-healthcheck": "^0.1.0",
        "express-winston": "^2.1.1",
        "hjs": "~0.0.6",
        "jade": "~1.7.0",
        "joi": "^11.1.1",
        "jquery": "^3.2.1",
        "less-middleware": "1.0.x",
        "lucene-escape-query": "^1.0.1",
        "lucene-parser": "^0.1.2",
        "morgan": "~1.0.0",
        "socket.io": "1.0.6",
        "static-favicon": "~1.0.0",
        "swagger-ui": "^3.2.0",
        "swagger-ui-standalone": "0.0.3",
        "uuid": "^3.1.0",
        "websocket": "~1.0.8",
        "winston": "^2.3.0",
        "yargs": "^8.0.2"
    },
    "devDependencies": {
        "axios": "^0.16.2",
        "chai": "^3.5.0",
        "chai-http": "^2.0.1",
        "chromedriver": "^2.32.2",
        "eslint": "^4.6.1",
        "eslint-config-godaddy": "^2.0.0",
        "eslint-plugin-json": "^1.2.0",
        "eslint-plugin-mocha": "^4.11.0",
        "geckodriver": "^1.8.0",
        "mocha": "^2.5.3",
        "selenium-webdriver": "^3.5.0"
