{
  "env": {
    "node": true,
    "mocha": true
  },
  "extends": "godaddy",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "requireConfigFile": false
  },
  "rules": {
    "eqeqeq": ["error", "smart"],
    "max-statements": "off",
    "complexity": "off",
    "no-process-env": "off",
    "no-console": "off",
    "mocha/no-exclusive-tests": "error",
    "comma-dangle": "off",
    "quote-props": "off",
    "no-sync": "off",
    "camelcase": "off"
  }
}